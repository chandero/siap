-- Name: fn_potencia_medida(integer); Type: FUNCTION; Schema: siap; Owner: isaguser
--

CREATE FUNCTION siap.fn_potencia_medida(empr integer) RETURNS TABLE(aap_potencia character varying, aap_medida character varying, cantidad integer)
    LANGUAGE plpgsql
    AS $$
DECLARE
  var_potencia record;
  var_medida record;
  var_cantidad integer;
BEGIN
  FOR var_potencia IN (SELECT DISTINCT ad.aap_potencia FROM siap.aap_adicional ad
                       INNER JOIN siap.aap a ON a.aap_id = ad.aap_id
                       WHERE ad.aap_id <> 9999999 and a.empr_id = EMPR and a.esta_id <> 9
                       ORDER BY ad.aap_potencia
                      ) 
  LOOP
    FOR var_medida IN (SELECT DISTINCT a.aaco_id, co.aaco_descripcion FROM siap.aap a
					   INNER JOIN siap.aap_conexion co ON co.aaco_id = a.aaco_id
		               WHERE a.aap_id <> 9999999 and a.empr_id = EMPR and a.esta_id <> 9
					   ORDER BY a.aaco_id
                     )
    LOOP
      SELECT COUNT(*) FROM siap.aap a INNER JOIN siap.aap_adicional ad ON ad.aap_id = a.aap_id
                      WHERE ad.aap_potencia = var_potencia.aap_potencia and a.aaco_id = var_medida.aaco_id and a.aap_id <> 9999999 and a.empr_id = EMPR and a.esta_id <> 9 INTO var_cantidad;
                      aap_potencia := var_potencia.aap_potencia::text;
                      aap_medida := var_medida.aaco_descripcion;
                      cantidad := var_cantidad;
                      RETURN NEXT;
 END LOOP;
END LOOP;
END; $$;


