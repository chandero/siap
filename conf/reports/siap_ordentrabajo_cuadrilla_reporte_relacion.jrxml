	<queryString language="SQL">
		<![CDATA[select r.* from (
	select DISTINCT ON (r.repo_consecutivo) r.*, rt.reti_descripcion, t.*, b.barr_descripcion, 
	o.orig_descripcion,  ((r.repo_fecharecepcion + interval '48h')::timestamp + (SELECT COUNT(*) FROM siap.festivo WHERE fest_dia BETWEEN r.repo_fecharecepcion and (r.repo_fecharecepcion + interval '48h')) * '1 day'::interval ) as fecha_limite, 
	c.cuad_descripcion, ot.ortr_consecutivo, ot.ortr_fecha from siap.reporte r 
                        left join siap.reporte_adicional a on r.repo_id = a.repo_id
                        left join siap.reporte_tipo rt on r.reti_id = rt.reti_id
                        left join siap.actividad t on a.acti_id = t.acti_id
                        left join siap.barrio b on r.barr_id = b.barr_id
                        left join siap.origen o on r.orig_id = o.orig_id
                        left join siap.ordentrabajo_reporte otr on otr.repo_id = r.repo_id
                        left join siap.ordentrabajo ot on ot.ortr_id = otr.ortr_id
                        left join siap.cuadrilla c on c.cuad_id = ot.cuad_id
                        where ot.ortr_fecha BETWEEN $P{FECHA_INICIAL} AND $P{FECHA_FINAL} and r.rees_id in (1,2) and r.empr_id = $P{EMPR_ID}  AND c.cuad_id = $P{CUAD_ID}
                        order by r.repo_consecutivo, ot.ortr_fecha DESC) r
 order by  r.reti_id, r.repo_id]]>
	</queryString>
